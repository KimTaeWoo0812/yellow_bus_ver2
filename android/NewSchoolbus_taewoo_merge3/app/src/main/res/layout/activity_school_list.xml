<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">



    <fragment
        android:id="@+id/heroImageView"
        android:name="net.computeering.newschoolbus.NewLogPackage.CalendarViewFragment"
        android:layout_marginLeft="5sp"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:scaleType="fitCenter"/>

    <ListView
    android:id="@+id/mList"
    android:layout_width="match_parent"
    android:layout_height="250dp"
    android:divider="#9E9E9E"
    android:dividerHeight="1dp"
    android:scrollbars="none"
        android:layout_gravity="bottom"></ListView>

    <TextView
        android:id="@+id/stickyView"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#222"
        android:gravity="center_vertical"
        android:paddingLeft="10dp"
        android:text="조회 가능한 학원 목록"
        android:textColor="#fff"
        android:textSize="20sp"
        android:textStyle="bold" />



</FrameLayout>